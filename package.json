{
  "name": "myapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "concurrently --kill-others-on-fail \"yarn start --reset-cache\" \"react-native run-android && react-native log-android\" \"yarn stay-awake-android\"",
    "ios": "concurrently --kill-others-on-fail \"yarn start --reset-cache\" \"react-native run-ios\"",
    "start": "react-native start",
    "web": "craco start",
    "build": "craco build",
    "test": "craco test --env=jsdom",
    "lint": "tsc && eslint '**/*.[jt]s?(x)' --cache --fix",
    "clean": "rm -rf node_modules && yarn && concurrently --kill-others-on-fail \"yarn clean-android\" \"yarn clean-ios\"",
    "clean-android": "cd android && rm -rf ./android/build && rm -rf ./android/.gradle && rm -rf ./android/app/build && ./gradlew clean && cd ..",
    "clean-ios": "rm -rf ~/Library/Developer/Xcode/DerivedData && cd ios && rm -rf build Pods && pod install && cd ..",
    "generate": "plop --plopfile ./.plop/plop.config.js"
  },
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "0.64.1",
    "react-native-web": "^0.16.2",
    "react-scripts": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/runtime": "^7.14.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.5",
    "@types/react-native": "^0.64.4",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-react-native-web": "^0.16.2",
    "concurrently": "^6.0.2",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-native-typescript": "^2.2.6",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "metro-react-native-babel-preset": "^0.66.0",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^2.0.0",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
